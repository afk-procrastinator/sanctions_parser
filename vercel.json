{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Set-Cookie": "SameSite=Lax; Secure; HttpOnly"
      }
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 